% --- SINGLE SLIDE: 4C-seq Preprocessing Pipeline (R/Bioconductor) ---
\begin{frame}{4C-seq Preprocessing Pipeline (R/Bioconductor)}
\begin{columns}[T]
  \column{0.45\textwidth}
  \footnotesize
  \textbf{Overview.}
  \begin{itemize}\itemsep2pt
    \item \textbf{Setup:} \texttt{Basic4Cseq}, \texttt{ShortRead}, \texttt{Biostrings}, \texttt{Rsubread}
    \item \textbf{Read \& QC:} list FASTQ, \texttt{qa()}, GC\%
    \item \textbf{Quality filter:} mean Phred $\geq$20 (report \%)
    \item \textbf{Adapter trimming:} \texttt{trimLRPatterns()}
    \item \textbf{Genome index:} \texttt{buildindex(memory=2000)}
    \item \textbf{Align:} \texttt{align(PE=TRUE, unique=TRUE)}
    \item \textbf{Post-process:} \texttt{sortBam()}, \texttt{indexBam()}
  \end{itemize}

  \medskip
  \textbf{Takeaway.} QC + adapter trimming + efficient indexing $\Rightarrow$ clean BAMs.

  \column{0.55\textwidth}
  \scriptsize
  \textbf{Key R functions.}
  \begin{itemize}\itemsep2pt
    \item \texttt{readFastq()} — FASTQ reading
    \item \texttt{qa()} — QC
    \item \texttt{quality()} — Phred scores
    \item \texttt{rowMeans()} — Avg. quality
    \item \texttt{trimLRPatterns()} — Adaptor cleaning
    \item \texttt{buildindex()} — genom index
    \item \texttt{align()} — alignment $\rightarrow$ BAM
    \item \texttt{sortBam()} — BAM sorting
    \item \texttt{indexBam()} — BAM indexing
  \end{itemize}
\end{columns}
\end{frame}
