\begin{frame}{Limited-Data Case Study: Results / Data Analysis and Bioinformatics}
    \begin{columns}[c] % The "c" option specifies centered vertical alignment while the "t" option is used for top vertical alignment
  \setbeamertemplate{caption}{\insertcaption}
        \column{.45\textwidth} % Left column and width
        \textbf{Key Findings}
        \begin{enumerate}
     \item Identified regulators of apoptosis, mitochondrial stress, and lipid metabolism under ND+Baf,
    \item Scoring results across treatment groups \textbf{validated by wet-lab assays }(PARP cleavage, PINK1, lipid droplets),
   
\end{enumerate}


        \column{.45\textwidth} % Right column and width
       \begin{figure}
        \centering
        \includegraphics[width=\linewidth]{parp.png}
        \caption{{\tiny\textit{Figure 8: Wet Lab: Western blot results show the accumulation of apoptotic signaling via PARP cleavage under different conditions. Increased PARP cleavage is indicative of apoptosis. However, under the ND+Baf condition, although PARP cleavage was elevated, the cells remained viable. Beta actin was used as the loading control.}}}
        \label{fig:placeholder}
    \end{figure}

    \end{columns}
\end{frame}
%------------------------------------------------
\begin{frame}{Limited-Data Case Study: Results / Data Analysis and Bioinformatics}
    \begin{columns}[c] % The "c" option specifies centered vertical alignment while the "t" option is used for top vertical alignment
  \setbeamertemplate{caption}{\insertcaption}
        \column{.45\textwidth} % Left column and width
        \textbf{Key Findings}
        \begin{enumerate}
     \item Identified regulators of apoptosis, mitochondrial stress, and lipid metabolism under ND+Baf ,
    \item Scoring results across treatment groups validated by wet-lab assays (PARP cleavage, PINK1, lipid droplets),
    \item \textbf{\alert{Fact:} Dry-lab} analyses revealed \textbf{synergistic treatment effects} and \textbf{guided experimental validation},
   
\end{enumerate}
        \column{.45\textwidth} % Right column and width
          \begin{figure}
        \centering
        \includegraphics[width=\linewidth]{figure_3_PPIs down_quadrant.jpg}
