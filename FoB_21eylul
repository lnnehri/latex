\begin{frame}{Limited-Data Case Study: Methods / Data Analysis and Bioinformatics}
    \begin{enumerate}
       
  \item Binary DE on bulk RNA-seq (per treatment)
  \item Integrated external omics (proteomics, scRNA-seq, mCRC bulk)
  \item Curated pathway sets for regulator discovery
  \item Processed in-house bulk; filtered and mapped to sets
  \textbf{\item Ran the enrichment pipeline across all omics}
  
    \end{enumerate}
\end{frame}
%------------------------------------------------
\begin{frame}{Limited-Data Case Study: Methods / Data Analysis and Bioinformatics}
    \begin{enumerate}
       
  \item Binary DE on bulk RNA-seq (per treatment)
  \item Integrated external omics (proteomics, scRNA-seq, mCRC bulk)
  \item Curated pathway sets for regulator discovery
  \item Processed in-house bulk; filtered and mapped to sets
  \item Ran the enrichment pipeline across all omics
  \item \textbf{PPI network analysis; pathway/co-regulation annotation}
 
    \end{enumerate}
\end{frame}
%------------------------------------------------
\begin{frame}{Limited-Data Case Study: Methods / Data Analysis and Bioinformatics}
    \begin{enumerate}
       
  \item Binary DE on bulk RNA-seq (per treatment)
  \item Integrated external omics (proteomics, scRNA-seq, mCRC bulk)
  \item Curated pathway sets for regulator discovery
  \item Processed in-house bulk; filtered and mapped to sets
  \item Ran the enrichment pipeline across all omics
  \item PPI network analysis; pathway/co-regulation annotation
 \textbf{ \item Pathway activity scoring across groups}
  
    \end{enumerate}
\end{frame}
%------------------------------------------------
\begin{frame}{Limited-Data Case Study: Methods / Data Analysis and Bioinformatics}
    \begin{enumerate}
       
  \item Binary DE on bulk RNA-seq (per treatment)
  \item Integrated external omics (proteomics, scRNA-seq, mCRC bulk)
  \item Curated pathway sets for regulator discovery
  \item Processed in-house bulk; filtered and mapped to sets
  \item Ran the enrichment pipeline across all omics
  \item PPI network analysis; pathway/co-regulation annotation
  \item Pathway activity scoring across groups
  \item \textbf{Synergy algorithms for combined-treatment effects}
    \end{enumerate}
\end{frame}
