\begin{frame}{Limited-Data Case Study: Results / Data Analysis and Bioinformatics}
    \begin{columns}[c] % The "c" option specifies centered vertical alignment while the "t" option is used for top vertical alignment
  \setbeamertemplate{caption}{\insertcaption}
        \column{.45\textwidth} % Left column and width
        \textbf{Key Findings}
        \begin{enumerate}
     \item Identified regulators of \textbf{apoptosis, mitochondrial stress, and lipid metabolism} under \textbf{nutrient restriction and lysosomal alkalization treatment} (ND+Baf),
   
\end{enumerate}


        \column{.45\textwidth} % Right column and width
         \begin{figure}
        \centering
        \includegraphics[width=\linewidth]{output (2).png}
        \caption{{\tiny\textit{Figure 7: Dry Lab: Group-wise comparison of core apoptosis regulators.}}}
        \label{fig:placeholder}
    \end{figure}
    \end{columns}
\end{frame}
