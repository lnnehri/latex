%------------------------------------------------
%------------------------------------------------
% --- SLIDE 1: Simulation I (with Death; PT grows less) ---
\begin{frame}{Markov Simulations — I (with Death)}
\begin{columns}[T]
  \column{0.40\textwidth}
  \footnotesize
  \textbf{Setup.} 4-state Markov chain: PT, M, ST, Death (absorbing). \\
  Transition $P$ as in code; initial $[100,0,0,0]$. \\
  Growth rates: PT=1, M=0, ST=2, Death=0. Steps=10.

  \medskip
  \textbf{Key result.}
  \begin{itemize}\itemsep2pt
    \item \textbf{Primary tumor grows less} overall; starvation drives loss.
    \item ST expands (higher growth), Death accumulates via absorption.
    \item Proportional share of PT declines over time.
  \end{itemize}

  \medskip
  \textbf{Outputs.} Proportions, total cells, dead cells.

  \column{0.60\textwidth}
  \centering
  \includegraphics[width=\linewidth]{firstmarkov.png}\\
  {\tiny \textit{Figure~17. Proportions over time (Simulation I).}}
\end{columns}
\end{frame}
%------------------------------------------------
% --- SLIDE 2: Simulation II (enhanced growth; PT grows more but share declines) ---
\begin{frame}{Markov Simulations — II (enhanced growth)}
\begin{columns}[T]
  \column{0.40\textwidth}
  \footnotesize
  \textbf{Setup.} Same $P$; initial $[10000,0,0,0]$. \\
  Growth rates: PT=10, M=0, ST=12, Death=0. Steps=20.

  \medskip
  \textbf{Key result.}
  \begin{itemize}\itemsep2pt
    \item \textbf{Primary tumor cell count increases} (faster growth).
    \item However, ST grows even faster $\Rightarrow$ \textbf{PT proportion decreases}.
    \item Death continues to accumulate; total population rises sharply.
  \end{itemize}

  \medskip
  \textbf{Outputs.} Proportions, total cells, dead cells.

  \column{0.60\textwidth}
  \centering
  \includegraphics[width=\linewidth]{secondmarkov.png}\\
  {\tiny \textit{Figure~18. Proportions over time (Simulation II).}}
\end{columns}
\end{frame}
%------------------------------------------------
% --- SLIDE 3: Simulation III (long-term; PT grows more but over longer horizon) ---
\begin{frame}{Markov Simulations — III (long-term)}
\begin{columns}[T]
  \column{0.40\textwidth}
  \footnotesize
  \textbf{Setup.} Same as Simulation II; Steps=100.

  \medskip
  \textbf{Key result.}
  \begin{itemize}\itemsep2pt
    \item \textbf{Primary tumor continues to grow}, but over a \textbf{longer horizon}.
    \item ST dominance persists; \textbf{PT proportion remains lower} long-term.
    \item Death increases monotonically due to absorbing state.
  \end{itemize}

  \medskip
  \textbf{Interpretation.} Faster PT growth can raise counts, yet relative burden shifts toward ST/metastatic compartments over time.

  \column{0.60\textwidth}
  \centering
  \includegraphics[width=\linewidth]{longtermmarkov.png}\\
  {\tiny \textit{Figure~19. Proportions over time (Simulation III).}}
\end{columns}
\end{frame}
%------------------------------------------------

\begin{frame}{Limited-Data Case Study: Results / Simulations}
    \begin{columns}[c]
    \setbeamertemplate{caption}{\insertcaption}

        \column{.45\textwidth}
        \textbf{Key Insights for Designing Future Experiments }
        \begin{enumerate}
    \item Markov models predict future cell state dynamics under continued treatment,
    \item Bayesian networks \textbf{uncover causal links between stress signals and phenotype transitions},
   
\end{enumerate}


        \column{.55\textwidth}
        \begin{figure}
            \centering
            \includegraphics[width=\linewidth]{bayesina.png}
            \textit{\caption{\tiny{Figure 20: Bayesian network of parameters to link causal relationships of survival, elongation, or death.}}}
            \label{fig:placeholder}
        \end{figure}

    \end{columns}
\end{frame}
%------------------------------------------------
% --- SLIDE 2: COMPLEX BAYESIAN NETWORK ---
\begin{frame}{Bayesian Network (Example)}
\begin{columns}[T]
  \column{0.35\textwidth}
  \footnotesize
  \textbf{Structure.}
  Starvation $\rightarrow$ ROS, Lipid Oxidation;\;
  Lysosomal Alkalinization $\rightarrow$ Ca$^{2+}$ Signaling;\;
  Ca$^{2+}$, ROS, Lipid Oxidation $\rightarrow$ Cell Phenotype.

  \medskip
 

  \textbf{Queries.}
  \[
    P(CP\mid S{=}1,L{=}1), \qquad
    P(CP\mid S{=}1,L{=}0)
  \]
  Compare phenotype distributions across evidence settings.

  \medskip
  \textbf{Diagnostics.}
  Print edges \& CPDs; visualize $P(CP\mid \cdot)$ as a heatmap.
 \textbf{Factorization.}
  \[
  \begin{aligned}
    P(&S,L,ROS,LO,Ca,CP)=
    P(S)\,P(L)\,P(ROS\mid S)\,P(LO\mid S)\\
    &\times P(Ca\mid L)\,P(CP\mid Ca,ROS,LO)
  \end{aligned}
  \]
  \column{0.52\textwidth}
  \centering
  \includegraphics[width=\linewidth]{Figure6_bayesian CPT.png}\\
  
\end{columns}
\end{frame}

%------------------------------------------------
\begin{frame}{Limited-Data Case Study: Results / Simulations}
    \begin{columns}[c]
    \setbeamertemplate{caption}{\insertcaption}

        \column{.45\textwidth}
        \textbf{Key Insights for Designing Future Experiments }
        \begin{enumerate}
    \item Markov models predict future cell state dynamics under continued treatment,
    \item Bayesian networks uncover causal links between stress signals and phenotype transitions,
   
\end{enumerate}


        \column{.45\textwidth}
         \item \textbf{\alert{Lesson:} }\textbf{Simulations provide testable hypotheses, guiding the design of new experiments}.

    \end{columns}
    
\end{frame}

