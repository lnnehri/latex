\begin{frame}{Data-Rich Case Study: Methods / Analysis Pipeline}
    \begin{enumerate}
  
  \item Hierarchical clustering on cell-type proportions $\Rightarrow$ immune-high vs epithelial-dominant
  \item Dimensionality reduction (PCA/UMAP) to visualize/validate separation
  \item Stats: Welch’s $t$ / Mann–Whitney U for differential cell types
  \item Correlation analysis among immune and epithelial populations
  \item Filtered reliably expressed genes per cluster
  \textbf{\item Classified genes as cluster-specific vs common (stability-based)}
 


    \end{enumerate}
\end{frame}


%------------------------------------------------

\begin{frame}{Data-Rich Case Study: Methods / Analysis Pipeline}
    \begin{enumerate}
  
  \item Hierarchical clustering on cell-type proportions $\Rightarrow$ immune-high vs epithelial-dominant
  \item Dimensionality reduction (PCA/UMAP) to visualize/validate separation
  \item Stats: Welch’s $t$ / Mann–Whitney U for differential cell types
  \item Correlation analysis among immune and epithelial populations
  \item Filtered reliably expressed genes per cluster
  \item Classified genes as cluster-specific vs common (stability-based)
  \textbf{\item Compared expressed-gene counts (Macro, DC, EpiT)}



    \end{enumerate}
\end{frame}


%------------------------------------------------

\begin{frame}{Data-Rich Case Study: Methods / Analysis Pipeline}
    \begin{enumerate}
  
  \item Hierarchical clustering on cell-type proportions $\Rightarrow$ immune-high vs epithelial-dominant
  \item Dimensionality reduction (PCA/UMAP) to visualize/validate separation
  \item Stats: Welch’s $t$ / Mann–Whitney U for differential cell types
  \item Correlation analysis among immune and epithelial populations
  \item Filtered reliably expressed genes per cluster
  \item Classified genes as cluster-specific vs common (stability-based)
  \item Compared expressed-gene counts (Macro, DC, EpiT)
  \item \textbf{Prioritized cluster-specific DE genes for annotation, enrichment, and PPI}


    \end{enumerate}
\end{frame}
