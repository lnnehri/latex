\begin{frame}{Bullet Points}

\begin{enumerate}
     \item Multi-omics $\Rightarrow$ testable wet-lab hypotheses (beyond static profiles)
  \item \textbf{Modeling prioritizes key signals; focuses limited wet-lab effort}
 
\end{enumerate}
\end{frame}
%------------------------------------------------
\begin{frame}{Bullet Points}

\begin{enumerate}
   \item Multi-omics $\Rightarrow$ testable wet-lab hypotheses (beyond static profiles)
  \item Modeling prioritizes key signals; focuses limited wet-lab effort
  \item \textbf{Context-aware models (cell type varies by tissue/disease/time)}
  
    
\end{enumerate}
\end{frame}
%------------------------------------------------
\begin{frame}{Bullet Points}
\begin{enumerate}

  \item Multi-omics $\Rightarrow$ testable wet-lab hypotheses (beyond static profiles)
  \item Modeling prioritizes key signals; focuses limited wet-lab effort
  \item Context-aware models (cell type varies by tissue/disease/time)
  \textbf{\item Small wet-lab phenotypes $\rightarrow$ feedback to simulations; boosts predictivity}
\end{enumerate}
\end{frame}
%------------------------------------------------
\begin{frame}{Bullet Points}

\begin{enumerate}

  \item Multi-omics $\Rightarrow$ testable wet-lab hypotheses (beyond static profiles)
  \item Modeling prioritizes key signals; focuses limited wet-lab effort
  \item Context-aware models (cell type varies by tissue/disease/time)
  \item Small wet-lab phenotypes $\rightarrow$ feedback to simulations; boosts predictivity
  \item \textbf{Coordinated dry+wet lab}\textbf{ cuts trial-and-error, accelerates discovery, tailors to the system}


\end{enumerate}
\end{frame}

